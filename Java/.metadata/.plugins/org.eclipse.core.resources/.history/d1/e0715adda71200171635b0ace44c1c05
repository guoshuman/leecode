	//时间复杂度O(n2)，空间复杂度O(1)
	public static int[] twoSum1(int[] nums, int target) 
	{
        for(int i=0;i<nums.length;i++)
        {
        	for (int j=i;j<nums.length;j++)
        		if(nums[i]+nums[j]==target)
        		{
        			return new int[]{nums[i],nums[j]};
        		}
        }
    }
	
