import java.awt.List;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashSet;


public class TwoSum {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		TwoSum sum = new TwoSum();
		int[] arr = {1,2,3};
		twoSum(arr,4);
		//System.out.println(Arrays.toString());
		
	}
	public static int[] twoSum(int[] nums, int target) {
		ArrayList list = new ArrayList();
		List<Integer> intList = Arrays.asList(nums);
		for (int i=0;i<nums.length;i++)
		{
			if (intList.contains(nums[i],target - nums[i]))
			{
				list.add(nums[i]);
				list.add(target - nums[i]);
			}
		}
		HashSet<String> set = new HashSet<String>(list);
		ArrayList<String> mid = new ArrayList<String>(set);
		String[] result = new String[mid.size()];
		mid.toArray(result);
		int[] re = new int[result.length];
		for (int i=0;i<result.length;i++)
		{
			re[i] = Integer.parseInt(result[i]);
		}
		return re;
        
    }
	
}

