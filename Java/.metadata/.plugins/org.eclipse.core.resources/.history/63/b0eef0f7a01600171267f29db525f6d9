/*
 * Determine whether an integer is a palindrome. Do this without extra space.
 */


public class Solution {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}
	public static boolean isPalindrome(int x){
		if(x<0)
			return false;
		int len=10;
		if((x/len)<10)
			len*=10;
		
		return true;
	}
}
